{
  "name": "Get Out Of My Way",
  "version": "0.1",
  "description": "Destroy anything on the page by clicking on it",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage", "tabs"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["GOFMW.js"],
    "persistent": false
  }
}
